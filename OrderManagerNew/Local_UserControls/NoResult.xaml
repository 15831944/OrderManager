<UserControl
  x:Class="OrderManagerNew.Local_UserControls.NoResult" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:OrderManagerNew.Local_UserControls" xmlns:local1="clr-namespace:OrderManagerNew"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" d:DesignHeight="610" d:DesignWidth="700"
  mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary Source="/StyleDictionary.xaml" />
    </UserControl.Resources>
    <Grid Height="610">
        <Image
          Height="160" HorizontalAlignment="Center" VerticalAlignment="Center"
          Opacity="0.6"
          Source="{DynamicResource image_searchDrawingImage}" />
        <Label
          HorizontalAlignment="Center" VerticalAlignment="Center"
          Content="{local1:LocExtension NoDataFound}"
          FontSize="32" Foreground="White" Opacity="0.6" />
    </Grid>
</UserControl>
